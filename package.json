        "prettier-plugin-svelte": "^3.1.2",
        "prettier-plugin-tailwindcss": "^0.5.7",
        "svelte": "^4.2.8",
        "svelte-check": "^3.6.2",
        "svelte-copy": "^1.4.1",
        "svelte-hero-icons": "^5.0.0",
        "tailwindcss": "^3.3.5",
        "tslib": "^2.6.2",
        "typescript": "^5.3.2",
        "uuid": "^9.0.1",
