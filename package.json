        "@sveltejs/adapter-node": "^1.3.1",
        "@sveltejs/kit": "^1.27.6",
        "@tailwindcss/forms": "^0.5.7",
        "@tailwindcss/typography": "^0.5.10",
        "@trivago/prettier-plugin-sort-imports": "^4.3.0",
        "@types/cookie": "^0.6.0",
        "@types/uuid": "^9.0.7",
