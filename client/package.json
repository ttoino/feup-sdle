        "postcss-load-config": "^5.0.2",
        "prettier": "^3.2.4",
        "prettier-plugin-svelte": "^3.1.2",
        "prettier-plugin-tailwindcss": "^0.5.11",
        "svelte": "^4.2.9",
        "svelte-check": "^3.6.3",
        "svelte-copy": "^1.4.1",
        "svelte-hero-icons": "^5.0.0",
        "tailwindcss": "^3.4.1",
        "tslib": "^2.6.2",
        "typescript": "^5.3.3",
        "uuid": "^9.0.1",
        "vite": "^5.0.12",
        "vitest": "^1.2.1",
